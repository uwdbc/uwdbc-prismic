{
  "format": "custom",
  "id": "navbar",
  "label": "Navbar",
  "repeatable": false,
  "status": true,
  "json": {
    "Main": {
      "links_to_social": {
        "type": "Group",
        "config": {
          "label": "links to social",
          "repeat": true,
          "fields": {
            "link": {
              "type": "Link",
              "config": {
                "label": "link",
                "placeholder": "",
                "select": null,
                "allowTargetBlank": false,
                "allowText": false,
                "repeat": false
              }
            },
            "link_type": {
              "type": "Select",
              "config": {
                "label": "link type",
                "placeholder": "",
                "options": [
                  "Instagram",
                  "Discord",
                  "Linktree",
                  "Email",
                  "Membership"
                ],
                "default_value": "Instagram"
              }
            }
          }
        }
      },
      "pages": {
        "type": "Group",
        "config": {
          "label": "Pages",
          "repeat": true,
          "fields": {
            "page": {
              "type": "Link",
              "config": {
                "label": "Page",
                "select": "document",
                "repeat": false,
                "customtypes": ["gallery", "home", "merch", "exec_page"]
              }
            },
            "display_name": {
              "type": "Text",
              "config": {
                "label": "Display Name",
                "placeholder": ""
              }
            },
            "sub_pages": {
              "type": "Text",
              "config": {
                "label": "Sub Pages",
                "placeholder": "write the id of the sub pages as you wrote them in the page with spaces in between."
              }
            }
          }
        }
      }
    }
  }
}
